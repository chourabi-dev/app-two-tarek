<div class="latest-blog-area pad-60">
    <div class="container">
        <div class="row">
            <div class="section-title">
                <h2>Latest Blog</h2>
                <div class="title-icon">
                    <span><i class="fa fa-angle-left"></i> <i class="fa fa-angle-right"></i></span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="blog-curosel">
                
            </div>
        </div>
    </div>
</div>

<section class="blog-area mb--70 mb-md--50">
    <div class="container">
        <div class="row mb--35 mb-md--23">
            <div class="col-12 text-center">
                <h2>News &amp; Updates</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="element-carousel" data-slick-options='{
                    "spaceBetween": 30,
                    "slidesToShow": 3,
                    "slidesToScroll": 1
                }'
                data-slick-responsive='[
                    {"breakpoint": 992, "settings": {"slidesToShow": 2}},
                    {"breakpoint": 768, "settings": {"slidesToShow": 1}}
                ]'>


                

                {% for article in [0..9] %}
                    {% include "components/blog-article-card.html.twig" %}
                {% endfor %}
                  
                </div>
            </div>
        </div>
    </div>
</section>